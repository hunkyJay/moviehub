#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../grunt-contrib-less/node_modules/maxmin/node_modules/strip-ansi/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../grunt-contrib-less/node_modules/maxmin/node_modules/strip-ansi/cli.js" "$@"
  ret=$?
fi
exit $ret
